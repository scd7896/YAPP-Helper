{
  "eslint.validate": [
    "html",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "eslint.options": {
    "extensinons": [
      ".js",
      ".ts",
      ".tsx"
    ]
  },
  "eslint.alwaysShowStatus": true,
  "eslint.packageManager": "npm",
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.formatOnSave": true,
  "prettier.semi": true,
  "prettier.useTabs": false,
  "prettier.tabWidth": 2,
  "prettier.printWidth": 80,
  "prettier.javascriptEnable": [
    "javascript",
    "javascriptreact"
  ]
}