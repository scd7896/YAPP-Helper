{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "utils/*": ["./src/utils/*"]
    },
    "sourceMap": false,
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "types": ["node"],
    "jsx": "react",
    "typeRoots": ["./src/types", "./src/action"]
  },
  "exclude": ["node_modules", "build"]
}
