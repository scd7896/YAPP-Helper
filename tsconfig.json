{
  "compilerOptions": {
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "utils/*": ["./src/utils/*"]
    },
    "sourceMap": true,
    "module": "esnext",
    "target": "es6",
    "jsx": "react",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "types": ["node"],
    "typeRoots": ["./src/types", "./action"]
  },
  "exclude": ["node_modules"]
}
