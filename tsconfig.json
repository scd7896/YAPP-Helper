{
  "compilerOptions": {
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "utils/*": ["./src/utils/*"],
      "pages": ["./src/pages/*"],
      "atomic/*": ["./src/component/atomic/*"],
      "molecules/*": ["./src/component/molecules/*"],
      "organisms": ["./src/component/organisms"],
      "template/*": ["./src/component/template/*"],
      "hooks/*": ["./src/hooks/*"],
      "actions/*": ["./src/store/action/*"]
    },
    "sourceMap": true,
    "module": "esnext",
    "target": "es6",
    "jsx": "react",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "types": ["node"],
    "typeRoots": ["./src/types", "./action"]
  },
  "exclude": ["node_modules"]
}
